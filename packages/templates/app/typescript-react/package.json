{
  "name": "templates-app-typescript-react",
  "description": "Polywrap App TypeScript React Template",
  "private": true,
  "version": "0.10.0-pre.7",
  "workspaces": {
    "nohoist": [
      "react-scripts",
      "react-scripts/**"
    ]
  },
  "dependencies": {
    "@polywrap/client-js": "0.10.0-pre.7",
    "@polywrap/react": "0.10.0-pre.6",
    "http-proxy": "1.18.1",
    "http-proxy-middleware": "1.0.6",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-lottie": "1.2.3",
    "react-scripts": "3.4.1",
    "react-toastify": "7.0.4",
    "stream-array": "1.1.2",
    "typescript": "4.1.6"
  },
  "devDependencies": {
    "@types/node": "12.12.26",
    "@types/react": "16.9.0",
    "@types/react-dom": "16.9.0",
    "@types/react-lottie": "1.2.5",
    "polywrap": "0.10.0-pre.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "prestart": "yarn codegen",
    "build": "react-scripts build",
    "prebuild": "yarn codegen",
    "codegen": "npx polywrap codegen",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
